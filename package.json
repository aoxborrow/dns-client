{
  "name": "@aoxborrow/dns-client",
  "version": "0.1.6",
  "description": "Fully-typed DNS resolver with support for UDP, TCP, and DNS-over-HTTPS (DoH) transports",
  "author": "Aaron Oxborrow <aaron@oxborrow.com>",
  "license": "MIT",
  "homepage": "https://github.com/aoxborrow/dns-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoxborrow/dns-client.git"
  },
  "bugs": {
    "url": "https://github.com/aoxborrow/dns-client/issues"
  },
  "keywords": [
    "dns",
    "dns-client",
    "dns-resolver",
    "doh",
    "dig",
    "dns-over-https",
    "dnssec",
    "authoritative",
    "dns-query",
    "recursive",
    "udp",
    "tcp",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "npm run tsc && npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint --cache --cache-location .eslintcache .",
    "eslint:fix": "eslint --cache --cache-location .eslintcache . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write --list-different .",
    "ts": "npm run lint",
    "ts:fix": "npm run lint:fix",
    "tsc": "tsc --noEmit",
    "typecheck": "npm run tsc",
    "typecheck:verbose": "tsc --noEmit --listFiles --diagnostics",
    "test": "jest --verbose",
    "test:path": "jest --verbose --testPathPattern",
    "test:name": "jest --verbose --testNamePattern",
    "test:path:debug": "jest --verbose --runInBand --bail=1 --testPathPattern",
    "test:name:debug": "jest --verbose --runInBand --bail=1 --testNamePattern",
    "test:failed": "jest --verbose --onlyFailures",
    "test:retry": "jest --verbose --passWithNoTests --onlyFailures",
    "test:no-integration": "jest --noStackTrace --testPathPattern='^(?!.*\\.integration\\.test).*$'",
    "test:integration": "jest --testPathPattern='.*\\.integration\\.test.*$'",
    "clear:ts": "rm -rf tsconfig.tsbuildinfo",
    "clear:jest": "npx jest --clearCache",
    "clear:eslint": "rm -rf .eslintcache",
    "clear": "npm run clear:ts && npm run clear:jest && npm run clear:eslint",
    "flush": "npm run clear && npm run generate",
    "query": "tsx scripts/dns-client.ts",
    "generate-fixtures": "node -e \"const args = process.argv.slice(1); require('child_process').spawn('tsx', ['scripts/generate-fixtures.ts'].concat(args), {stdio: 'inherit'});\"",
    "release:patch": "npm version patch && git push --follow-tags && gh release create v$(node -p \"require('./package.json').version\") --generate-notes",
    "release:minor": "npm version minor && git push --follow-tags && gh release create v$(node -p \"require('./package.json').version\") --generate-notes",
    "release:major": "npm version major && git push --follow-tags && gh release create v$(node -p \"require('./package.json').version\") --generate-notes"
  },
  "dependencies": {
    "dns-packet": "^5.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/dns-packet": "^5.6.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
